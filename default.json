{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        "github>aquaproj/aqua-renovate-config#1.6.0",
        ":timezone(Asia/Tokyo)"
    ],
    "schedule": [
        "* 21-23 * * fri",
        "* * * * sat,sun"
    ],
    "prHourlyLimit": 0,
    "packageRules": [
        {
            "matchPackageNames": [
                "aquaproj/aqua-registry"
            ],
            "schedule": [
                "* 21-23 * * fri"
            ]
        }
    ]
}
